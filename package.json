{
  "name": "react-express-app",
  "version": "1.0.0",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "concurrently \"npm run server\" \"cd client & npm run build\""
  },
  "dependencies": {
    "amqplib": "^0.6.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "socket.io": "^3.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
